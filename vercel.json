{
  "version": 2,
  "functions": {
    "gomoku_ai_server.py": {
      "maxDuration": 60
    }
  },
  "builds": [
    {
      "src": "gomoku_ai_server.py",
      "use": "@vercel/python"
    },
    {
      "src": "index.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/ai_move",
      "dest": "gomoku_ai_server.py"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
