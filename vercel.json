{
  "version": 2,
  "builds": [
    {
      "src": "/api/gomoku_ai_server.py", 
      "use": "@vercel/python",          
      "config": {
        "maxLambdaSize": "50mb",       
         "runtime": "python3.9"       
      }
    }
  ],
  "routes": [
    {
      "src": "/api/gomoku_ai_server",   
      "dest": "/api/gomoku_ai_server.py" 
    },
    {
       "src": "/(.*)",                 
       "dest": "/index.html"          
       
    }
  ]
}