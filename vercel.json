{
  "version": 2,
  "builds": [
    {
      "src": "gomoku_ai_server.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaDuration": 60
      }
    },
    {
      "src": "index.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/ai_move",
      "dest": "gomoku_ai_server.py"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
